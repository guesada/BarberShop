{
  "name": "barbershop-enterprise",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8",
    "axios": "^1.6.8",
    "react-hook-form": "^7.51.1",
    "@tanstack/react-query": "^5.20.4",
    "zustand": "^4.5.0",
    "framer-motion": "^11.0.8",
    "react-hot-toast": "^2.4.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.344.0",
    "web-vitals": "^3.5.2",
    "react-redux": "^9.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:prod": "GENERATE_SOURCEMAP=false npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "clean": "rm -rf build node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "dev:https": "HTTPS=true npm start",
    "dev:debug": "REACT_APP_DEBUG=true npm start",
    "analyze": "npm run build && npx serve -s build",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=build",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:surge": "npm run build && surge build",
    "performance": "npm run build && lighthouse-ci autorun",
    "security": "npm audit && snyk test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/node-logger": "^7.6.17",
    "@storybook/preset-create-react-app": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/node": "^20.11.30",
    "webpack-bundle-analyzer": "^4.10.1",
    "lighthouse": "^11.7.0",
    "lighthouse-ci": "^0.12.0",
    "@lighthouse-ci/cli": "^0.12.0",
    "snyk": "^1.1291.0",
    "serve": "^14.2.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "npm-run-all": "^4.1.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "wait-on": "^7.2.0",
    "start-server-and-test": "^2.0.3",
    "cypress": "^13.7.1",
    "playwright": "^1.42.1",
    "@playwright/test": "^1.42.1",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.2.13",
    "@testing-library/jest-dom": "^6.4.2",
    "react-test-renderer": "^18.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "source-map-explorer": "^2.5.3",
    "why-did-you-render": "^8.0.1",
    "react-axe": "^3.5.4",
    "axe-core": "^4.8.4",
    "@axe-core/react": "^4.8.4",
    "bundlesize": "^0.18.1",
    "size-limit": "^11.1.1",
    "@size-limit/preset-app": "^11.1.1",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "workbox-cli": "^7.0.0"
  },
  "proxy": "http://localhost:3000",
  "homepage": ".",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "300 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "50 kB"
    }
  ]
}
